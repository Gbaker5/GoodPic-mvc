	<head>
		<title>Profile</title>
		<meta charset="utf-8" />
		<meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no" />
		<link rel="stylesheet" href="../assets/css/main.css" />
        <script src="https://kit.fontawesome.com/d7ccb43ad1.js" crossorigin="anonymous"></script>
	</head>
	<body class="landing is-preload">
		<div id="page-wrapper">

			<!-- Header -->
				<header id="header" class="alt">
					<h1><a href="index.html">GoodPic</a> by KalaniTech</h1>
					<nav id="nav">
						<ul>
							<li><a href="index.html">Home</a></li>
							<li>
								<a href="#" class="icon solid fa-angle-down"><i class="fa-solid fa-bars"></i></a>
								<ul>
                                    <div>
                                        <li><a><img class="profile-img-small" src="<%=profile[0].profilePic%>" alt="profile picture"></a></li>
                                        
                                    </div>
									<li><a href="/feed">Feed</a></li>
									<li><a href="/friends">Friends</a></li>
									
									
								</ul>
							</li>
							<li><a href="index.html" class="button">Logout</a></li>
						</ul>
					</nav>
				</header>

			<!-- Banner -->
				<section id="banner">
                    <section>
                        <img class="profile-img" src="<%=profile[0].profilePic%>" alt="profile picture">
                    </section>
					
					<p><%=bio.Nickname%></p>
					<ul class="actions special">
						
						<li><a href="/profileEdit" class="button">Edit <i class="fa-regular fa-pen-to-square"></i></a></li>
					</ul>
				</section>

			<!-- Main -->
				<section id="main" class="container">

					<section class="box special">
						<header class="major">
							<h2>Email: <%=thisUser.email%>
							<br />
							Username: <%=thisUser.userName%></h2>
							<p>Favorite Quote: "Blandit varius ut praesent nascetur eu penatibus nisi risus faucibus nunc ornare<br />
							adipiscing nunc adipiscing. Condimentum turpis massa."</p>
						</header>
						<span class="image featured"><img src="../img/cam1.webp" alt="" /></span>
					</section>

					<section class="box special features">
                        <div><a href="/initialBio">Edit Bio</a></div>
						<div class="features-row">
							<section>
								<span class="icon solid major fa-solid fa-signature accent2"></span>
								<h3>What do we call you?</h3>
								<p>Name:  <%=bio.Name%>
                                <br>Nickname: <%=bio.Nickname%>
                                </p>
							</section>
							<section>
								<span class="icon solid major fa-regular fa-star accent3"></span>
								<h3>Lets get astrological</h3>
								<p>Age: <%=bio.Age%>
                                    <br>
                                    Zodiac: <%=bio.Sign%>
                                </p>
							</section>
						</div>
						<div class="features-row">
							<section>
								<span class="icon solid major fa-regular fa-heart accent4"></span>
								<h3>Your favorites</h3>
								<p>Favorite Food: <%=bio.favoriteFood%>
                                    <br>Favorite Movie: <%=bio.favoriteMovie%>
                                    <br>Favorite Artist: <%=bio.favoriteArtist%>
                                    <br>Favorite Song: <%=bio.favoriteSong%>
                                </p>
							</section>
							<section>
								<span class="icon solid major fa-regular fa-face-grin-tongue-squint accent5"></span>
								<h3>Whats the coolest thing about you?</h3>
								<p>Coolest: <%=bio.Coolest%></p>
							</section>
						</div>
					</section>


                    <!-- Main -->
				<section  class="box newpost col-10">
                    <section class="box special">
                        <header>
                            <h2>Add a New Post</h2>
                            <p>Share your experiences with the world <i class="fa-regular fa-face-grin-wink"></i></p>
                        </header>
                    </section>
					
					<div class="box">
						
							<div class="row gtr-50 gtr-uniform">
							
                                <form action="/post/createPost"  enctype="multipart/form-data" method="POST">
         
                                    <div class="col-12">
                                      <label for="imageUpload" class="">
                                        
                                      <ul class="actions ">
                                        <li><input type="file" class="form-control button" id="imageUpload" name="file"/>
                                            <span class="uploadtext">Choose Upload File</span>    
                                        </li>
                                        
                                      </ul> 
                                        </label>
                                    </div>
                                    <div class="col-12">
                                        <p>This image must be a .jpg, jpeg, or .png file.</p>
                                    </div>

                                    <div class="mb-3 col-6">
                                        <label for="title" class="form-label">Title</label>
                                        <input type="text" class="form-control" id="title" name="title">
                                    </div>
                                    <div class="mb-3 col-12">
                                        <label for="caption" class="form-label">Caption</label>
                                        <input type="text" class="form-control" id="caption" name="caption">
                                    </div>
                                   
                                    <div>
                                        <ul class="actions special">
                                            <li><input type="submit" value="upload"/></li>
                                        </ul>
                                    </div>
                                </form>  
                                  
                               

							 
							
							</div>
						  
					</div>
				</section>

					<div class="row">
						<!--<div class="col-6 col-12-narrower">

							<section class="box special">
								<span class="image featured"><img src="../images/porsche2.jpg" alt="post" /></span>
								<h3>Porsche Panarera</h3>
								<p>Top speed: Faster than you</p>
								<ul class="actions special">
									<li><a href="post.html" class="button alt">View Post</a></li>
								</ul>
							</section>

						</div>-->

                        <% for(var i=0; i<posts.length; i++) {%>
						<div class="col-6 col-12-narrower">

							<section class="box special">
                                <!--<a href="/post/<%= posts[i]._id%>">
 
                                </a>-->
                                <span class="image featured"><img src="<%= posts[i].image%>" alt="post" /></span>
								
								<h3><%=posts[i].title%></h3>
								<p><%=posts[i].caption%></p>
								<ul class="actions special">
									<li><a href="/post/<%= posts[i]._id%>" class="button alt">View Post</a></li>
								</ul>
							</section>

						</div>
                        <% } %>

                        

					</div>

				</section>

			<!-- CTA -->
				<section id="cta">

					<ul class="actions special">
                        <li><a href="/feed" class="button alt">Return to Feed</a></li>
                    </ul>

				</section>

			<!-- Footer -->
				<footer id="footer">
					<ul class="icons">
						<li><a href="#" class="icon brands fa-twitter"><span class="label">Twitter</span></a></li>
						<li><a href="#" class="icon brands fa-facebook-f"><span class="label">Facebook</span></a></li>
						<li><a href="#" class="icon brands fa-instagram"><span class="label">Instagram</span></a></li>
						<li><a href="#" class="icon brands fa-github"><span class="label">Github</span></a></li>
						<li><a href="#" class="icon brands fa-dribbble"><span class="label">Dribbble</span></a></li>
						<li><a href="#" class="icon brands fa-google-plus"><span class="label">Google+</span></a></li>
					</ul>
					<ul class="copyright">
						<li>&copy; Untitled. All rights reserved.</li><li>Design: <a href="http://html5up.net">HTML5 UP</a></li>
					</ul>
				</footer>

		</div>

		<!-- Scripts -->
			<script src="../assets/js/jquery.min.js"></script>
			<script src="../assets/js/jquery.dropotron.min.js"></script>
			<script src="../assets/js/jquery.scrollex.min.js"></script>
			<script src="../assets/js/browser.min.js"></script>
			<script src="../assets/js/breakpoints.min.js"></script>
			<script src="../assets/js/util.js"></script>
			<script src="../assets/js/main.js"></script>

	</body>
