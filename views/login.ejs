<body class="is-preload">
    <div id="page-wrapper">

        <!-- Header -->
            <header id="header">
                <h1><a href="/">GoodPic</a> by KalaniTech</h1>
                <nav id="nav">
                    <ul>
                        <li><a href="/profile">Home</a></li>
                        <li>
